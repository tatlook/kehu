@define eat $apples from  @{
@}.
@define eat $apples from $piece @{
    for $apple in $apples @{
        print $apple to $stdout.
    @}.
    return .
@}.

@define main $args @{
    assign $mima to 1.
    inline "$mima += 1".
    set $mima to 4.
    eat [ 1 2 3 $mimi ] from $nil
    prin "great" to $stdout.
    print $thing @where $thing "yes".
@}.
